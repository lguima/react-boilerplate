{
  "plugins": [
    ["babel-plugin-styled-components", { "ssr": true, "displayName": true }],
    "babel-plugin-macros"
  ],
  "presets": [
    ["next/babel", { "preset-react": { "runtime": "automatic" } }],
    "@babel/preset-typescript"],
  "env": {
    "test": {
      "plugins": [
        ["babel-plugin-styled-components", { "ssr": false, "displayName": false }]
      ]
    }
  }
}
